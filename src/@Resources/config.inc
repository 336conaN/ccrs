VisualizerFrequencyResolution=1024
VisualizerSensitivity=35.0
CpuTempIndex=2
CpuUsageIndex=1
GpuTempIndex=6
GpuUsageIndex=7
DiskTempIndex=4
DiskUsageIndex=5
MemoryTempIndex=3
MemoryUsageIndex=0
