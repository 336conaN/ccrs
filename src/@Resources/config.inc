[Variables]
Scale=1.0
Theme=Default
BorderWidth=1
TextFont=SF Pro
NerdFont=RobotoMono Nerd Font Propo
FontWeightMain=600
FontWeightSub=400
CornerRadiusOutside=8
CornerRadiusInside=6
DisableContextMenu=false
OpenConfigOnClick=true
; WinFetchLogo=%USERPROFILE%\Pictures\Other\windows.png
