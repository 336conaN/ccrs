[mDriveTemp]
Measure=Registry
RegHKey=HKEY_CURRENT_USER
RegKey=SOFTWARE\HWiNFO64\VSB
RegValue=ValueRaw#DriveTempIndex#
MinValue=0
MaxValue=100
RegExpSubstitute=1
Substitute="^.*]:":""

