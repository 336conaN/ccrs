[mCpuTemp]
Measure=Registry
RegHKey=HKEY_CURRENT_USER
RegKey=SOFTWARE\HWiNFO64\VSB
RegValue=ValueRaw#CpuTempIndex#
MinValue=0
MaxValue=100
RegExpSubstitute=1
Substitute="^.*]:":""

[mCpuUsage]
Measure=Registry
RegHKey=HKEY_CURRENT_USER
RegKey=SOFTWARE\HWiNFO64\VSB
RegValue=ValueRaw#CpuUsageIndex#
MinValue=0
MaxValue=100
RegExpSubstitute=1
Substitute="^.*]:":""
